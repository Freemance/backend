{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "admin": {
      "type": "library",
      "root": "libs/admin",
      "entryFile": "index",
      "sourceRoot": "libs/admin/src",
      "compilerOptions": {
        "tsConfigPath": "libs/admin/tsconfig.lib.json"
      }
    },
    "client": {
      "type": "library",
      "root": "libs/client",
      "entryFile": "index",
      "sourceRoot": "libs/client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/client/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "assets": [
      {
        "include": "../templates/**/*",
        "outDir": "dist/templates"
      }
    ],
    "webpack": true
  }
}
